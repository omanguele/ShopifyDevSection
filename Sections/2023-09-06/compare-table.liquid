<section id="comp_tab63609032">
  <section class="container">
    <h1>{{ section.settings.heading }}</h1>
    <section class="tab_content">
      <div class="tab_content1">
        <h2 id="benefits">{{ section.settings.heading_compare }}</h2>
        {% assign compare_labels = section.settings.compare_labels | split: ',' %}
        {% for label in compare_labels %}
          <h3>{{ label }}</h3>
        {% endfor %}
      </div>
      <div class="tab_content2">
        <img src="{{ section.settings.image_dhl | img_url: "master" }}" alt="">
        <h2>{{ section.settings.heading_dhl }}</h2>
        {% assign compare_text_dhl = section.settings.compare_text_dhl | split: ',' %}
        {% for text in compare_text_dhl %}
          <h3>
            {% if text == 'pass' %}
              <svg
                xmlns="http://www.w3.org/2000/svg"
                viewBox="0 0 16 16"
                width="40px"
                height="40px"
                fill="{{ section.settings.check_icon_color }}"
                style="height:100%;"
              >
                <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z"></path>
              </svg>
            {% elsif text == 'fail' %}
              <svg
                xmlns="http://www.w3.org/2000/svg"
                viewBox="0 0 16 16"
                width="40px"
                height="40px"
                fill="currentColor"
                style="height:100%; "
              >
                <path d="M4.646 4.646a.5.5 0 0 1 .708 0L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 0 1 0-.708z"></path>
              </svg>
            {% else %}
              {{ text }}
            {% endif %}
          </h3>
        {% endfor %}
      </div>
      {% for block in section.blocks %}
        <div class="tab_content{{ forloop.index | plus: 2 }}">
          <img src="{{ block.settings.image | img_url: "master" }}" alt="">
          <h2>{{ block.settings.heading }}</h2>
          {% assign compare_text = block.settings.compare_text | split: ',' %}
          {% for text in compare_text %}
            <h3>
              {% if text == 'pass' %}
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  viewBox="0 0 16 16"
                  width="40px"
                  height="40px"
                  fill="{{ section.settings.check_icon_color }}"
                  style="height:100%;"
                >
                  <path d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z"></path>
                </svg>
              {% elsif text == 'fail' %}
