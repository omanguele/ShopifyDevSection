              <div class="tabcontent2">
                <img src="{{ block.settings.image | img_url: "master" }}" alt="">
                <h2>{{ block.settings.heading }}</h2>
                {% assign compare_text = block.settings.compare_text | split: ',' %}
                {% for text in compare_text %}
