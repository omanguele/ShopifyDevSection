<style>
  body {
      overflow: initial !important;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 {
      background-color: {{ section.settings.bg_color }};
      position: relative;
      width: 100%;
      padding: 100px 0;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_container {
      margin: 0 auto;
      max-width: 100%;
      width: 100%;
      padding-right: 15px;
      padding-left: 15px;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_heading {
      display: flex;
      margin-bottom: 55px;
      justify-content: center;
      align-items: center;
      text-align: center;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_heading {
      font-weight: 700;
      font-size: 31px;
      line-height: 120%;
      letter-spacing: 0px;
      text-transform: uppercase;
      color: {{ section.settings.text_color }};
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_wrapper {
      display: flex;
      justify-content: center;
      align-items: center;
      position: relative;
      flex-direction: column;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_wrapper_left {
      padding-right: 15px;
      padding-left: 15px;
      position: relative;
      display: flex;
      justify-content: center;
      align-items: center;
      flex-direction: column;
      gap: 30px;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_wrapper_right {
      min-height: 1px;
      padding-right: 15px;
      padding-left: 15px;
      position: relative;
      display: flex;
      align-items: flex-start;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_wrapper_right_list {
      width: 100%;
      padding-top: 30px;
      display: flex;
      flex-direction: column;
      justify-content: flex-start;
      align-items: flex-start;
      gap: 50px;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_wrapper_right_list_item {
      display: flex;
      justify-content: flex-start;
      align-items: flex-start;
      gap: 20px;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_icon_1 {
      width: 150px;
  }
   img {
      max-width: 100%;
      object-fit: cover;
      height: auto;
      width: 100%;
      display: block;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_img_g1 {
      width: 100%;
      border-radius: 10px;
      overflow: hidden;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_card {
      display: flex;
      background-color: {{ section.settings.card_color }};
      justify-content: center;
      width: 100%;
      align-items: center;
      flex-direction: column;
      padding: 35px 30px;
      border-radius: 10px;
      text-align: center;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_img_g2 {
      grid-template-columns: repeat(1, 1fr);
      width: 100%;
      display: grid;
      gap: 30px;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_img_g2 img {
      height: 100%;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_text_h {
      font-weight: 400;
      margin-bottom: 5px;
      color: {{ section.settings.text_color }};
      font-size: 12px;
      line-height: 144%;
      letter-spacing: .05em;
      text-transform: uppercase;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_text_d {
      letter-spacing: 0;
      color: {{ section.settings.text_color }};
      font-weight: 300;
      font-size: 12px;
      line-height: 136%;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_card_heading {
      margin-bottom: 25px;
      font-weight: 400;
      font-size: 12px;
      line-height: 144%;
      letter-spacing: .05em;
      text-transform: uppercase;
      color: {{ section.settings.text_color }};
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_card_list_icon {
      margin-bottom: 30px;
      display: flex;
      align-items: center;
      justify-content: center;
      width: 100%;
      gap: 10px;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_card_icon {
      height: 60px;
      width: 60px;
  }
  #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_card_icon img {
      width: 100%;
      height: 100%;
      object-fit: cover;
  }
  @media (min-width: 576px) {
      #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_container {
          max-width: 540px;
      }
      #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_img_g2 {
          grid-template-columns: repeat(2, 1fr);
      }
  }
  @media (min-width: 768px) {
      #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_container {
          max-width: 740px;
      }
      #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_wrapper_left,
      #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_wrapper_right {
          width: 84%;
      }
  }
  @media (min-width: 992px) {
      #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_container {
          max-width: 1130px;
      }
      #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_wrapper_left {
          width: 58%;
      }
      #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_wrapper_right {
          width: 42%;
      }
      #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_wrapper_right_list{
          padding-top: 0px;
          position: sticky;
          top: 100px;
      }
      #shopify-section-{{ section.id }} #imageicontext7171409032 .imageicontext7171409032_wrapper {
          flex-direction: row;
          align-items: stretch;
      }
  }
</style>

<section id="imageicontext7171409032">
  <div class="imageicontext7171409032_container">
    <div class="imageicontext7171409032_head">
      <h2 class="imageicontext7171409032_heading">
        {{ section.settings.heading }}
      </h2>
    </div>
    <div class="imageicontext7171409032_wrapper">
      <div class="imageicontext7171409032_wrapper_left">
        <div class="imageicontext7171409032_img_g1">
          <img src="{{ section.settings.image_1 | img_url : "master" }}" alt="">
        </div>
        <div class="imageicontext7171409032_img_g2">
          <div class="imageicontext7171409032_img_g1">
            <img src="{{ section.settings.image_2 | img_url : "master" }}" alt="">
          </div>
          <div class="imageicontext7171409032_img_g1">
            <img src="{{ section.settings.image_3 | img_url : "master" }}" alt="">
          </div>
        </div>
        <div class="imageicontext7171409032_card">
          <div class="imageicontext7171409032_card_heading">{{ section.settings.heading_card }}</div>
          <div class="imageicontext7171409032_card_list_icon">
            {% for block in section.blocks %}
              {% if block.type == 'icon' %}
                <div class="imageicontext7171409032_card_icon">
                  <img src="{{ block.settings.image | img_url : "master" }}" alt="">
                </div>
              {% endif %}
            {% endfor %}
          </div>
          <div class="imageicontext7171409032_text_d">
            {{ section.settings.description_card }}
          </div>
        </div>
      </div>
      <div class="imageicontext7171409032_wrapper_right">
        <div class="imageicontext7171409032_wrapper_right_list">
          {% for block in section.blocks %}
            {% if block.type == 'icon_text' %}
              <div class="imageicontext7171409032_wrapper_right_list_item">
                <div class="imageicontext7171409032_icon_1">
                  <img src="{{ block.settings.image | img_url : "master" }}" alt="">
                </div>
                <div class="imageicontext7171409032_text">
                  <div class="imageicontext7171409032_text_h">{{ block.settings.heading }}</div>
                  <div class="imageicontext7171409032_text_d">
                    {{ block.settings.description }}
                  </div>
                </div>
              </div>
            {% endif %}
          {% endfor %}
        </div>
      </div>
    </div>
  </div>
</section>

{% schema %}
{
  "name": "Img Icon Text",
  "blocks": [
    {
      "type": "icon_text",
      "name": "Icon Text",
      "settings": [
        {
          "type": "text",
          "id": "heading",
          "label": "Heading"
        },
        {
          "type": "textarea",
          "id": "description",
          "label": "Description"
        },
        {
          "type": "image_picker",
          "id": "image",
          "label": "image"
        }
      ]
    },
    {
      "type": "icon",
      "name": "Icon",
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "image"
        }
      ]
    }
  ],
  "settings": [
    {
      "type": "color",
      "id": "bg_color",
      "label": "Bg Color",
      "default": "#fff"
    },
    {
      "type": "color",
      "id": "card_color",
      "label": "Bg Color",
      "default": "#f2f2f2"
    },
    {
      "type": "color",
      "id": "text_color",
      "label": "Text Color",
      "default": "#000"
    },
    {
      "type":"image_picker",
      "id":"image_1",
      "label":"Image 1"
    },
    {
      "type":"image_picker",
      "id":"image_2",
      "label":"Image 2"
    },
    {
      "type":"image_picker",
      "id":"image_3",
      "label":"Image 3"
    },
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "Lounge"
    },
    {
      "type": "text",
      "id": "heading_card",
      "label": "Heading Card",
      "default": "Lounge"
    },
    {
      "type": "text",
      "id": "description_card",
      "label": "Description Card",
      "default": "Comfort. All Day. Every Day."
    }
  ],
  "presets": [
    {
      "name": "Img Icon Text"
    }
  ]
}
{% endschema %}
