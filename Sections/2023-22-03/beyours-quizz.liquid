                  "value": "{{ block.settings.text }}",
                  "info": "{{ block.settings.info }}",
                  "associated_products": [],
                },
              {% endif %}
          {% endfor %}
        ],
    },
   {
        "question_id": 'step-2',
        "question": "{{ section.settings.title-step-2 }}",
        "answer_type": "{{ section.settings.answer-type-step-2 }}",
        "button_text": "{{ section.settings.btn-text-step-2 }}",
        "answers": [
          {% for block in section.blocks %}
              {% if block.type == 'step-2' %}
                {
                  "type": "{{ block.settings.answer-type }}",
                  "path": {% if block.settings.image != nil %} "{{ block.settings.image | img_url:"master" }}" {% else %} "" {% endif %},
                  "value": "{{ block.settings.text }}",
                  "info": "{{ block.settings.info }}",
                  "associated_products": [],
                },
              {% endif %}
          {% endfor %}
        ],
    },
   {
        "question_id": 'step-3',
        "question": "{{ section.settings.title-step-3 }}",
        "answer_type": "{{ section.settings.answer-type-step-3 }}",
        "button_text": "{{ section.settings.btn-text-step-3 }}",
        "answers": [
          {% for block in section.blocks %}
              {% if block.type == 'step-3' %}
                {
                  "type": "{{ block.settings.answer-type }}",
                  "path": {% if block.settings.image != nil %} "{{ block.settings.image | img_url:"master" }}" {% else %} "" {% endif %},
                  "value": "{{ block.settings.text }}",
                  "info": "{{ block.settings.info }}",
                  "associated_products": [],
                },
              {% endif %}
          {% endfor %}
        ],
    },
   {
        "question_id": 'step-4',
        "question": "{{ section.settings.title-step-4 }}",
        "answer_type": "{{ section.settings.answer-type-step-4 }}",
        "button_text": "{{ section.settings.btn-text-step-4 }}",
        "answers": [
          {% for block in section.blocks %}
              {% if block.type == 'step-4' %}
                {
                  "type": "{{ block.settings.answer-type }}",
                  "path": {% if block.settings.image != nil %} "{{ block.settings.image | img_url:"master" }}" {% else %} "" {% endif %},
                  "value": "{{ block.settings.text }}",
                  "info": "{{ block.settings.info }}",
                  "associated_products": [],
                },
              {% endif %}
          {% endfor %}
        ],
    },
   {
        "question_id": 'step-5',
        "question": "{{ section.settings.title-step-5 }}",
        "answer_type": "{{ section.settings.answer-type-step-5 }}",
        "button_text": "{{ section.settings.btn-text-step-5 }}",
        "answers": [
          {% for block in section.blocks %}
              {% if block.type == 'step-5' %}
                {
                  "type": "{{ block.settings.answer-type }}",
                  "path": {% if block.settings.image != nil %} "{{ block.settings.image | img_url:"master" }}" {% else %} "" {% endif %},
                  "value": "{{ block.settings.text }}",
                  "info": "",
                  "associated_products": [
                    {% for p in block.settings.upsells %}
                      {
                        "id": "{{ p.id }}" ,
                        "url": "{{ p.url }}" ,
                        "name": "{{ p.title | escape }}" ,
                        "image": "{{ p.featured_image | product_img_url: "medium"}}",
