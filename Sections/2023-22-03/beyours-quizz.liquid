function renderProducts() {
      let html = '';
      selectedProducts.forEach((item, index) => {
          html += `
            <div class="ctn-prdt">
                <a href="${item.url}" class="prdt-image">
                    <img src="${item.image}" alt="${item.name}">
                </a>
                <a href="${item.url}" class="prdt-detail">
                    <div class="prdt-name">
                        <p>${item.name}</p>
                    </div>
                    <div class="prdt-price">
                       ${item.money}
                    </div>
                </a>
                <div class="prdt-selected">
                    <input type="checkbox" name="" value="${item.price}" class="in_rslt_prdt" id="prdt_${index}">
                    <label for="prdt_${index}">Sélectionnez ce produit</label>
                </div>
            </div>
          `;
      });
      document.querySelector('#shopify-section-{{ section.id }} #quizzpage280123 .wrap-prdts').innerHTML = html;
      let inRsltPrdts = document.querySelectorAll('#shopify-section-{{ section.id }} #quizzpage280123 .wrap-prdts .in_rslt_prdt');
      const htmlPrice = document.querySelector('#shopify-section-{{ section.id }} #quizzpage280123 .ctn-price .true_price');

      inRsltPrdts.forEach((item, index) => {
          item.addEventListener('click', function(e) {
              if(e.target.checked){
                 price = price  + parseFloat(e.target.value);
                 htmlPrice.innerHTML = `Total: ${price}€`;
              }else{
                price = price  - parseFloat(e.target.value);
                 htmlPrice.innerHTML = `Total: ${price}€`;
              }
          });
      });
    }

ansQuestStep5.forEach((ans, index) => {
    ans.addEventListener('click', function(e) {
        selectedProducts = data[4].answers[index].associated_products;
        renderProducts();
    });
});

nextBtns.forEach((clickBtn, clickIndex) => {

    clickBtn.addEventListener('click', function(e) {
        e.preventDefault();
        let isComplete = formItems[step].querySelector('.answers').classList.contains('complete');
        if(step == (data.length - 1)) {
            progressBar.style.display = 'none';
            containerForm.classList.add('hide');
            containerForm.style.display = 'none';

            loadScreen.style.display = 'inherit';
            setTimeout(() => {
                loadScreen.classList.remove('hide');
            }, 100);

            setTimeout(() => {
                loadScreen.classList.add('hide');
                loadScreen.style.display = 'none';

                ctnPrdts.style.display = 'flex';
                setTimeout(() => {
                    ctnPrdts.classList.add('show');
                }, 100);

            }, 5000);
            
        } else if(isComplete){
            formItems[step].classList.remove('active');
            formItems[step + 1].classList.add('active');
            setSelectElts();
            step = step + 1;
            progressBar.style.width = Math.round((100/data.length ) * step  ) + '%' ;
            progressBar.querySelector('span').innerHTML = Math.round((100/data.length ) * step ) + '%' ;
        }
    });

});

prevBtns.forEach((clickBtn, clickIndex) => {
    clickBtn.addEventListener('click', function(e) {
        e.preventDefault();
        formItems[step].classList.remove('active');
        formItems[step - 1].classList.add('active');
        setSelectElts(); 
        step = step - 1; 
        progressBar.style.width = Math.round((100/data.length ) * step  ) + '%' ;
        progressBar.querySelector('span').innerHTML = Math.round((100/data.length ) * step ) + '%' ;
    })
});

function setSelectElts() {
    let elts = document.querySelectorAll('#shopify-section-{{ section.id }} #quizzpage280123  .form-item.active .answer-item label');
    elts.forEach((elt) => {
        elt.addEventListener('click', function(e) {
            setTimeout(() => {
                let isCompleteRadio = document.querySelectorAll('#shopify-section-{{ section.id }} #quizzpage280123  .form-item.active .answer-item input[type="radio"]:checked');
                let isCompleteChekbox = document.querySelectorAll('#shopify-section-{{ section.id }} #quizzpage280123  .form-item.active .answer-item input[type="checkbox"]:checked');
                if(isCompleteRadio.length > 0 || isCompleteChekbox.length > 0){
                    document.querySelector('#shopify-section-{{ section.id }} #quizzpage280123  .form-item.active .answers').classList.add('complete');
                }else{
                    document.querySelector('#shopify-section-{{ section.id }} #quizzpage280123  .form-item.active .answers').classList.remove('complete');
                }
            }, 100)
            
        })
    })
}

const firstScreen = document.querySelector('#shopify-section-{{ section.id }} #quizzpage280123  .first-screen');
document.querySelector('#shopify-section-{{ section.id }} #quizzpage280123  .start__btn').addEventListener('click', function(e) {
    e.preventDefault();
    firstScreen.classList.add('hide');
    setTimeout(() => {
        firstScreen.style.display = 'none';
    }, 100);

    containerForm.style.display = 'flex';
    setTimeout(() => {
        containerForm.classList.remove('hide');
    }, 100);

});

setSelectElts();
</script>

