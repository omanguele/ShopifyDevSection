];

let quizzform = document.querySelector('#shopify-section-{{ section.id }} #quizzpage280123  #quizzform');
let step = 0;
let html = '';

data.forEach((item, index, arr) => {
    let active = index == 0 ? 'active' : '';
    html += `<div class="form-item ${active}">
                <div class="form-wrap">
                    <h1 class="question">${item.question}</h1>
                    <div class="answers ">`;
    
    item.answers.forEach((answer, ansIndex) =>  {
        html += `<div class="answer-item">`;
        html +=  item.answer_type == 'single' ? 
                `<input type="radio" name="${item.question_id}" id="${item.question_id}_${ansIndex+1}" value="${answer.value}">` : 
                `<input type="checkbox" name="${item.question_id}" id="${item.question_id}_${ansIndex+1}" value="${answer.value}">`;
        

        if(answer.type == 'text') {
          html += `<label for="${item.question_id}_${ansIndex+1}" class="${item.question_id}" data-id="${ansIndex}">`;
          html += answer.info != '' ?
                      `<span class="ico-info">
                            <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor">
                                <path stroke-linecap="round" stroke-linejoin="round" d="M12 9v3.75m9-.75a9 9 0 11-18 0 9 9 0 0118 0zm-9 3.75h.008v.008H12v-.008z" />
                                </svg>                                          
                        </span>
                        <span class="info">${answer.info}</span>
                        ` : ``;
          html += answer.path != '' ? 
                      `<span>
                            <img src="${answer.path}" class="img_illust"/>
                        </span>` : ``;
          html += `<span>${answer.value}</span>
                    </label>`;
          
        }else{
            html += `<label for="${item.question_id}_${ansIndex+1}" class="ctn-ans-image ${item.question_id}" data-id="${ansIndex}">
                        <div class="ans-image">
                            <img src="${answer.path}" alt="">
                        </div>
                        <span>${answer.value}</span>
                    </label>
                `;
        }
        html += `</div>`;
    });

    html += `</div><div class="ctn-btn">`;
