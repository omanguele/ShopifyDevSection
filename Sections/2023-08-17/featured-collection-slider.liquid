<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@10/swiper-bundle.min.css">

<section id="featuredtabcollectionsliderxb25318">
  <div class="featuredtabcollectionsliderxb25318_container">
    <h2 class="featuredtabcollectionsliderxb25318_title">{{ section.settings.heading }}</h2>
    <div class="featuredtabcollectionsliderxb25318_tab_container">
      <div class="featuredtabcollectionsliderxb25318_tab_header" role="tablist">
        {% for block in section.blocks %}
          <div
            class="featuredtabcollectionsliderxb25318_tab_header_item"
            {% if forloop.index == 1 %}
              aria-selected="true"
            {% endif %}
          >
            {{ block.settings.tab_label }}
          </div>
        {% endfor %}
        <span class="indicator"></span>
        <span class="scrollbottom"></span>
      </div>
      <div class="featuredtabcollectionsliderxb25318_tab_body">
        {% for block in section.blocks %}
          <div
            {% if forloop.index == 1 %}
              aria-selected="true"
            {% endif %}
            class="swiper featuredtabcollectionsliderxb25318_tab_body_item featuredtabcollectionsliderxb25318_tab_body_item_1"
          >
            <div class="swiper-wrapper">
              <div class="swiper-slide featuredtabcollectionsliderxb25318_slide featured">
                <div class="featuredtabcollectionsliderxb25318_ft_slide_inner">
                  <div class="featuredtabcollectionsliderxb25318_featured_overlay"></div>
                  <div class="featuredtabcollectionsliderxb25318_featured_prdt_count">
                    {{ block.settings.collection.products.size }}
                    {{ block.settings.count_suffix }}
                  </div>
                  <div class="featuredtabcollectionsliderxb25318_featured_ctn_btn">
                    <a href="{{ block.settings.btn_url }}" class="featuredtabcollectionsliderxb25318_featured_btn">
                      <span>
                        <span>{{ block.settings.btn_label }}</span>
                      </span>
                    </a>
                  </div>
                  <div
                    class="featuredtabcollectionsliderxb25318_featured_image"
                    style="background-image: url({{ block.settings.image | img_url : "master" }});"
                  ></div>
                </div>
              </div>
              {% for product in block.settings.collection.products %}
                <div class="swiper-slide featuredtabcollectionsliderxb25318_slide">
                  <a href="{{ product.url }}" class="featuredtabcollectionsliderxb25318_slide_inner">
                    <div class="featuredtabcollectionsliderxb25318_prdt_image">
                      <div class="prdt_badge">limited</div>
                      <img
                        src="{{ product.featured_image | product_img_url: "master" }}"
                        alt=""
                      >
                    </div>
                    <div class="featuredtabcollectionsliderxb25318_prdt_info">
                      <p class="prdt_title">{{ product.title | escape }}</p>
                      <p class="prdt_price">
                        {{ product.price_min | money }}
                        <s>{{ product.price_max | money }}</s>
                      </p>
                    </div>
                  </a>
                </div>
              {% endfor %}
            </div>
            <div class="swiper-button-prev"></div>
            <div class="swiper-button-next"></div>
          </div>
        {% endfor %}
      </div>
    </div>
  </div>
</section>
