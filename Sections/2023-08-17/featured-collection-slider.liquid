<script>
  
  
  
  
        const tabsHeader = document.querySelector(
            '#shopify-section-{{ section.id }} #featuredtabcollectionsliderxb25318 .featuredtabcollectionsliderxb25318_tab_header');
        const tabsHeaderItems = Array.from(tabsHeader.querySelectorAll(
            '.featuredtabcollectionsliderxb25318_tab_header_item'));
        const tabsBodyItems = Array.from(document.querySelectorAll(
            '#shopify-section-{{ section.id }} #featuredtabcollectionsliderxb25318 .featuredtabcollectionsliderxb25318_tab_body .featuredtabcollectionsliderxb25318_tab_body_item'
        ));
        const indicator = tabsHeader.querySelector('.indicator');
        let activeHeaderItems = tabsHeader.querySelector('[aria-selected]');

        if (activeHeaderItems) {
            indicator.style.setProperty('transform', getTransform(activeHeaderItems))
        }

 
        function onItemHeaderClick(e, index) {
            if (e.currentTarget === activeHeaderItems) {
                return;
            }

            if (activeHeaderItems) {
                activeHeaderItems.removeAttribute('aria-selected');
                e.currentTarget.setAttribute('aria-selected', 'true')


                indicator.animate([{
                    transform: getTransform(e.currentTarget)
                }], {
                    fill: 'both',
                    duration: 600,
                    easing: 'cubic-bezier(.48,1.55,.28,1)'
                });

                activeHeaderItems = e.currentTarget;

                tabsBodyItems.forEach((bItem, bIndex) => {
                    if (index === bIndex) {
                        bItem.setAttribute('aria-selected', 'true');
                    } else {
                        bItem.removeAttribute('aria-selected');
                    }
                });
            }
        }


        function getTransform(element) {
            const transform = {
                x: element.offsetLeft,
                scaleX: element.offsetWidth / 100,
            };

            return `translateX(${transform.x}px) scaleX(${transform.scaleX})`;
        }

        tabsHeaderItems.forEach((hItem, hIndex) => {
            new Swiper(`#shopify-section-{{ section.id }} #featuredtabcollectionsliderxb25318 .featuredtabcollectionsliderxb25318_tab_body_item_${hIndex+1}`, {
                slidesPerView: 'auto',
                spaceBetween: 24,
                freeMode: true,
                grab: true,
                navigation: {
                    nextEl: `#shopify-section-{{ section.id }} #featuredtabcollectionsliderxb25318 .featuredtabcollectionsliderxb25318_tab_body_item_${hIndex+1} .swiper-button-next`,
                    prevEl: `#shopify-section-{{ section.id }} #featuredtabcollectionsliderxb25318 .featuredtabcollectionsliderxb25318_tab_body_item_${hIndex+1} .swiper-button-prev`,
                },
            });
            hItem.addEventListener('click', (e) => {
                onItemHeaderClick(e, hIndex)
            });
        })




</script>

